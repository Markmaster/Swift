let имяШаблон = "Имя: "
let фамилияШаблон = "Фамилия: "
let отчествоШаблон = "Отчество: "
let ростШаблон = "Рост: "
let весШаблон = "Вес: "
let возрастШаблон = "Возраст: "

var имяСтудента = "Иван"
var фамилияСтудента = "Петрович"
var отчествоСтудента = "Голопопов"
var ростСтудента = "173"
var весСтудента = "99"
var возрастСтудента = "45"

print (имяШаблон + имяСтудента)
print (фамилияШаблон + фамилияСтудента)
print (отчествоШаблон + отчествоСтудента)
print (ростШаблон + ростСтудента + " см")
print (весШаблон + весСтудента + " кг")
print (возрастШаблон + возрастСтудента)
